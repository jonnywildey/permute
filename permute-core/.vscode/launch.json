{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'permute'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=permute"
        ],
        "filter": {
          "name": "permute",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'permute'",
      "cargo": {
        "args": [
          "build",
          "--bin=permute",
          "--package=permute"
        ],
        "env": {
          "RUST_BACKTRACE": "1"
        },
        "filter": {
          "name": "permute",
          "kind": "bin"
        }
      },
      "args": [
        "--file=examples/beep24.wav",
        "--files=examples/amen.wav",
        "--trimAll",
        "--output",
        "./renders/",
        "--inputTrail=0",
        "--outputTrail=2",
        "--permutations=1",
        "--depth=1",
        "--normalise",
        "--processorCount=1",
        "--processor=Sidechain Gain"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'permute'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=permute",
          "--package=permute"
        ],
        "filter": {
          "name": "permute",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}